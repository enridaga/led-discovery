<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="WARN">
      <AppenderRef ref="Console"/>
    </Root>
    <Logger name="led.discovery.nlp.LemmaCleanerTest" level="warn">
      <AppenderRef ref="Console"/>
    </Logger>
    <Logger name="led.discovery.nlp.StanfordNLPTest" level="warn">
      <AppenderRef ref="Console"/>
    </Logger>
  </Loggers>
</Configuration>